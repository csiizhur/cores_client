<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="124.0" prefWidth="350.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.lanen.util.messager.Confirm">
  <!-- TODO Add Nodes -->
  <children>
    <ImageView fitHeight="124.0" fitWidth="350.0" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@confirm.jpg" />
      </image>
    </ImageView>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
        <Button mnemonicParsing="false" onAction="#onCancelBtnAction" prefHeight="26.0" prefWidth="65.0" text="否(N)" AnchorPane.bottomAnchor="8.0" AnchorPane.rightAnchor="12.0" />
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#onOkBtnAction" prefHeight="26.0" prefWidth="65.0" text="是(Y)" AnchorPane.bottomAnchor="8.0" AnchorPane.rightAnchor="86.0" />
        <Label fx:id="label1" layoutX="54.0" layoutY="17.0" prefWidth="284.0" text="如何改变文件扩展名，可能会导致文件不可用" />
        <Label fx:id="label2" layoutX="54.0" layoutY="52.0" prefWidth="284.0" text="确定要更改吗？" />
      </children>
    </AnchorPane>
  </children>
</StackPane>
