package com.lanen.model.clinicaltest;


import javafx.beans.property.SimpleStringProperty;
import javafx.beans.property.StringProperty;

/**
 * 检疫数据表格 用,成员变量与tableView列对应.
 * @author Administrator
 *
 */
public class XyshTableData {
	private StringProperty animalCode;         //动物编号
    private StringProperty specimenCode;       //检验编号
   
    private StringProperty collectionTime;       //数据采集时间(检验时间)
    
    //血生化指标
    private StringProperty bhao;
    private StringProperty ast;
    private StringProperty alt;
    private StringProperty tp;
    private StringProperty alb;
    private StringProperty ggt;
    private StringProperty tbil;
    private StringProperty bun;
    private StringProperty crea;
    private StringProperty glu;
    private StringProperty tg;
    private StringProperty chol;
    private StringProperty ldh;
    private StringProperty ck;
    private StringProperty na;
    private StringProperty k;
    private StringProperty ci;
   
    public XyshTableData(){
    	
    }
    public XyshTableData(    		
    		String bhao,String ast,String alt,String tp,String alb,String ggt,String tbil,String bun,String crea,String glu,String tg,String chol,String ldh,String ck,String na,String k,String ci,         
    		      
    		String animalCode,    
    		String specimenCode,  
    		String collectionTime
    		){
    		this.bhao=new SimpleStringProperty(bhao);
    	    this.ast         =  new SimpleStringProperty(ast);
    	    this.alt=new SimpleStringProperty(alt);
    	    this.tp=new SimpleStringProperty(tp);
    	    this.alb=new SimpleStringProperty(alb);
    	    this.ggt=new SimpleStringProperty(ggt);
    	    this.tbil=new SimpleStringProperty(tbil);
    	    this.bun=new SimpleStringProperty(bun);
    	    this.crea=new SimpleStringProperty(crea);
    	    this.glu=new SimpleStringProperty(glu);
    	    this.tg=new SimpleStringProperty(tg);
    	    this.chol=new SimpleStringProperty(chol);
    	    this.ldh=new SimpleStringProperty(ldh);
    	    this.ck=new SimpleStringProperty(ck);
    	    this.na=new SimpleStringProperty(na);
    	    this.k=new SimpleStringProperty(k);
    	    this.ci=new SimpleStringProperty(ci);
    	    this.animalCode		=  new SimpleStringProperty(animalCode	)  ;
    	    this.specimenCode	=  new SimpleStringProperty(specimenCode)  ;
    	    
    	    this.collectionTime	=  new SimpleStringProperty(collectionTime);
    	    
    	
    }
    
    
	public String getAst() {
		return ast.get();
	}
	public void setAst(String ast) {
		this.ast =  new SimpleStringProperty(ast);
	}
	
	
	public String getAnimalCode() {
		return animalCode.get();
	}
	public void setAnimalCode(String animalCode) {
		this.animalCode =  new SimpleStringProperty(animalCode);
	}
	public String getSpecimenCode() {
		return specimenCode.get();
	}
	public void setSpecimenCode(String specimenCode) {
		this.specimenCode =  new SimpleStringProperty(specimenCode);
	}
	
	public String getCollectionTime() {
		return collectionTime.get();
	}
	public void setCollectionTime(String collectionTime) {
		this.collectionTime = new SimpleStringProperty(collectionTime);
	}
	public String getBhao() {
		return bhao.get();
	}
	public void setBhao(String bhao) {
		this.bhao = new SimpleStringProperty(bhao);
	}
	public String getAlt() {
		return alt.get();
	}
	public void setAlt(String alt) {
		this.alt = new SimpleStringProperty(alt);
	}
	public String getTp() {
		return tp.get();
	}
	public void setTp(String tp) {
		this.tp = new SimpleStringProperty(tp);
	}
	public String getAlb() {
		return alb.get();
	}
	public void setAlb(String alb) {
		this.alb = new SimpleStringProperty(alb);
	}
	public String getGgt() {
		return ggt.get();
	}
	public void setGgt(String ggt) {
		this.ggt = new SimpleStringProperty(ggt);
	}
	public String getTbil() {
		return tbil.get();
	}
	public void setTbil(String tbil) {
		this.tbil = new SimpleStringProperty(tbil);
	}
	public String getBun() {
		return bun.get();
	}
	public void setBun(String bun) {
		this.bun = new SimpleStringProperty(bun);
	}
	public String getCrea() {
		return crea.get();
	}
	public void setCrea(String crea) {
		this.crea = new SimpleStringProperty(crea);
	}
	public String getGlu() {
		return glu.get();
	}
	public void setGlu(String glu) {
		this.glu = new SimpleStringProperty(glu);
	}
	public String getTg() {
		return tg.get();
	}
	public void setTg(String tg) {
		this.tg = new SimpleStringProperty(tg);
	}
	public String getChol() {
		return chol.get();
	}
	public void setChol(String chol) {
		this.chol = new SimpleStringProperty(chol);
	}
	public String getLdh() {
		return ldh.get();
	}
	public void setLdh(String ldh) {
		this.ldh = new SimpleStringProperty(ldh);
	}
	public String getCk() {
		return ck.get();
	}
	public void setCk(String ck) {
		this.ck = new SimpleStringProperty(ck);
	}
	public String getNa() {
		return na.get();
	}
	public void setNa(String na) {
		this.na =new SimpleStringProperty( na);
	}
	public String getK() {
		return k.get();
	}
	public void setK(String k) {
		this.k = new SimpleStringProperty(k);
	}
	public String getCi() {
		return ci.get();
	}
	public void setCi(String ci) {
		this.ci =new SimpleStringProperty( ci);
	}       
    
}
